    "dayjs": "^1.10.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "helmet": "^4.3.1",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.14.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
