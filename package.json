    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.20",
    "@types/passport-jwt": "^3.0.3",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.6",
